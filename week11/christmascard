<script type="text/javascript">

	$("#fourone").text()
$("#fourone").fadeOut(30)
$("#fourone").fadeIn(8000)

$("#fourtwo").text()
$("#fourtwo").fadeOut(30)
$("#fourtwo").fadeIn(8000)

$("#rudolph").text()
