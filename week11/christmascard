$("#rudolph").fadeIn(8000)





</script>
<script> 
	var leftOffSet = 0

	var moveHeading = function(){
		$("#clouds").offset({left: leftOffSet})

		leftOffSet++

		if (leftOffSet > 500){
			leftOffSet = 50
		}
	}

		setInterval(moveHeading, 5)

</script>
